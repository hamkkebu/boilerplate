# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=auth_service
MYSQL_ROOT_PASSWORD=your_secure_password_here

# JWT Configuration (IMPORTANT: Change in production! Must be at least 64 characters for HS512)
JWT_SECRET=ThisIsADevelopmentSecretKeyPleaseChangeInProductionAtLeast64Characters

# Security Configuration
LOGIN_MAX_ATTEMPTS=20
LOGIN_BLOCK_DURATION=3
RATE_LIMIT_AUTH_REQUESTS=1000
RATE_LIMIT_GENERAL_REQUESTS=5000

# gRPC Configuration
GRPC_SERVER_PORT=9090

# Application Configuration
SPRING_PROFILES_ACTIVE=dev
